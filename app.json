{
  "expo": {
    "name": "Logistigas Motorista",
    "slug": "logistigas-driver",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-logistigas2.png",
    "scheme": "logistigasdriver",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.REQUEST_INSTALL_PACKAGES",
        "android.permission.WAKE_LOCK"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/icon-logistigas2.png",
        "backgroundImage": "./assets/images/icon-logistigas2.png",
        "monochromeImage": "./assets/images/icon-logistigas2.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.caioeliasss.logistigasdriver"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon-logistigas2.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon-logistigas2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permita que o app acesse sua localização em segundo plano.",
          "locationAlwaysPermission": "Permita que o app acesse sua localização em segundo plano.",
          "locationWhenInUsePermission": "Permita que o app acesse sua localização.",
          "isAndroidBackgroundLocationEnabled": true
        }
      ],
      "expo-asset",
      {
        "assets": [
          "./assets/images/icon-logistigas2.png",
          "./assets/images/icon-logistigas.png"
        ]
      }
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cf2c3ae2-bcd2-441d-809f-93a197b95eb5"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/cf2c3ae2-bcd2-441d-809f-93a197b95eb5"
    }
  }
}
